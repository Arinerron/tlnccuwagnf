reporter_ifel => fn(b, unevaluated x, unevaluated y) {
  ret => '';
  ifel(b, fn() {
    ret -> x();
  }, fn() {
    ret -> y();
  });
  return(ret);
};

x => fn() {
  print("this should not get called");
  return("bad");
};

y => fn() {
  print("this should get called");
  return("good");
};

result => reporter_ifel(false, x(), y());
print(result);
